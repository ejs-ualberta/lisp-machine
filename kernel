	ldr :rr r1 :AVL_Node.left
	jnc :rr :bp state_1
	ldr :rr r1 :AVL_Node.right
	jnc :rr :bp state_1
state_2
	ldr r3 r1 :AVL_Node.data
	sbs r3 r3 r2
