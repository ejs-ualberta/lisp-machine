	ads :rr :sp -1
	str r1 :rr 0
	str r2 :rr 1
	:>[:callb r3]
	ads :rr :rr 1
	shf :rr :rr -2
	jnc :sr :pc :rr

option1
	ldr r1 r1 :AVL_Node.left
	jnc r1 :bp avl_find_loop1
	xor :rr :rr :rr
	jnc :sr :bp avl_find_cleanup

option2
	ads :rr r1 0
	jnc :sr :bp avl_find_cleanup
	0 0

option3
	ldr r1 r1 :AVL_Node.left
	jnc r1 :bp avl_find_loop1
	xor :rr :rr :rr

avl_find_cleanup
	ldr :bp :fp 5
	ldr r3 :fp 4
	ldr r2 :fp 3
	ldr r1 :fp 2
	ldr :lr :fp 1
	ldr :fp :fp 0
	sbs :sp :sp 8
	:ret

NODE 0 0 0 0
]]

